cat("\nNaive Model Accuracy Metrics:\n")
f2 <- naive(ts, h=60)
acc_f2 <- accuracy(f2)
print(acc_f2)
cat("\nSeasonal Naive Model Accuracy Metrics:\n")
f3 <- snaive(ts, h=60)
acc_f3 <- accuracy(f3)
print(acc_f3)
cat("\nRandom Walk with Drift Model Accuracy Metrics:\n")
f4 <- rwf(ts, drift=TRUE, h=60)
acc_f4 <- accuracy(f4)
print(acc_f4)
