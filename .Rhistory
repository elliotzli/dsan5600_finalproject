ARIMA = acc_pred[1, ],
Mean = acc_f1[1, ],
Naive = acc_f2[1, ],
SeasonalNaive = acc_f3[1, ],
Drift = acc_f4[1, ]
)
all_acc <- t(all_acc)
lowest_scores <- apply(all_acc, 2, function(x) {
names(which.min(x))
})
cat("\nModel with the best Accuracy Metrics:\n",lowest_scores)
